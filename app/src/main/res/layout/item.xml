<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="10dp"
    android:layout_margin="10dp"
    app:cardElevation="10dp"
    android:backgroundTint="#D6D0D0">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="90dp"
                android:text="Name :"
                android:textColor="#000"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/nm"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:text="Name"
                android:textSize="20dp" />

        </LinearLayout>
        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="90dp"
                android:text="Contact :"
                android:textSize="20dp"
                android:textColor="#000"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginLeft="5dp"
                android:layout_width="wrap_content"
                android:text="Name"
                android:id="@+id/ct"
                android:textSize="20dp"
                android:layout_height="wrap_content"/>


        </LinearLayout>
        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="90dp"
                android:text="Email ID:"
                android:textSize="20dp"
                android:textColor="#000"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/el"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="Name"
                android:textSize="20dp" />

        </LinearLayout>
        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="90dp"
                android:text="Adress :"
                android:textColor="#000"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginLeft="5dp"
                android:layout_width="wrap_content"
                android:text="Name"
                android:id="@+id/as"
                android:textSize="20dp"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="90dp"
                android:text="City:"
                android:textColor="#000"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/cy"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:text="Name"
                android:textSize="20dp" />

        </LinearLayout>
        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:text="Qualification:"
                android:textColor="#000"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginLeft="5dp"
                android:layout_width="wrap_content"
                android:text="Name"
                android:id="@+id/kl"
                android:textSize="20dp"
                android:layout_height="wrap_content"/>

        </LinearLayout>

        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:text="Click Button Calling"
                android:textSize="20dp"
                android:textColor="#001C0F"
                android:layout_height="wrap_content"/>

            <TextView
                android:layout_marginLeft="50dp"
                android:layout_width="40dp"
                android:id="@+id/call"
                android:background="@drawable/calling_icon"
                android:textSize="20dp"
                android:layout_height="30dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:textSize="10dp"
                android:id="@+id/latitude"
                android:textColor="#001C0F"
                android:visibility="gone"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginLeft="25dp"
                android:layout_width="wrap_content"
                android:textSize="20dp"
                android:id="@+id/longit"
                android:visibility="gone"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginLeft="25dp"
                android:layout_width="wrap_content"
                android:textSize="20dp"
                android:textColor="#000"
                android:text="Live Location"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/maps"
                android:layout_marginLeft="87dp"
                android:background="@drawable/loctioni"
                />


        </LinearLayout>


    </LinearLayout>
</androidx.cardview.widget.CardView>